{
  "name": "phapi/cache-redis",
  "description": "Cache provider using Redis as backend",
  "keywords": [
    "cache", "redis", "phapi"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "phapi/contract": "1.*",
    "phapi/exception": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.6.*",
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\Cache\\Redis\\": "src/Phapi/Cache/Redis/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Cache\\Redis\\": "tests/Phapi/Cache/Redis/"
    }
  }
}